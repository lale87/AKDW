% Literaturverzeichnis
%  Eintrag wird erst dann angezeigt, sobald dieser einmal mit \cite referenziert
%  wurde.
%  (Verweis auf Eintrag: \cite{KEY}, Verweis mit Seitenzahl: \cite[2--4]{KEY}
% http://de.wikibooks.org/wiki/LaTeX-Kompendium:_Zitieren_mit_BibTeX

% Buch
@Book{Example01,
    Title       = {Buchtitel},
    Author      = {Max Mustername and John Doe},
    Publisher   = {Verlag},
    Address     = {Ort},
    Year        = {1992},
    
    Edition     = {X}
}

% Artikel
@Article{Example02,
    Title       = {Titel des Artikels},
    Author      = {Autor},
    Journal     = {Journal-Titel},
    Year        = {1992}
}

% Webseite
@Electronic{Example03,
    Title       = {Titel der Webseite},
    Author      = {Autor},
    Month       = {01},
    Year        = {1992},
    Url         = {http://www.example.de/},
    Note        = {Abgerufen am 20.08.2014}
}